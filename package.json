  "repository": {
    "type": "git",
    "url": "git+https://github.com/lambda2/meshtastic-catalogs.git"
  },
  "type": "module",
  "author": "lambda2",
  "dependencies": {
    "change-case": "^5.4.4",
    "deep-object-diff": "^1.1.9",
    "deepmerge": "^4.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.2",
    "@types/chrome": "^0.0.263",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.9",
    "jest": "^29.7.0",
    "microbundle": "^0.15.1",
    "ts-jest": "^29.2.5",
    "tslib": "^2.6.3",
    "tsx": "^4.19.2",
    "typescript": "^5.5.2"

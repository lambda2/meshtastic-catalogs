{
  "name": "meshtastic-catalogs",
  "version": "1.0.9",
  "description": "Ready-to-use configurations for meshtastic devices",
  "license": "GPL-3.0-only",
  "scripts": {
    "lint": "pnpm biome format . --fix",
    "build": "pnpm prebuild && microbundle",

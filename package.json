{
  "name": "@lambda2/meshtastic-configs",
  "version": "1.0.0",
  "main": "index.js",
  "description": "Browser library for interfacing with meshtastic devices",
  "license": "GPL-3.0-only",
  "scripts": {
    "lint": "pnpm biome format . --fix",
    "build": "tsup && pnpm biome format .",
    "generate:docs": "typedoc src/index.ts",
    "prebuild": "tsx src/index.ts",
    "test": "jest"
  },
  "keywords": [
    "meshtastic library typescript javascript"
  ],
  "publishConfig": {
    "provenance": true
  },
  "sideEffects": false,
  "packageManager": "pnpm@9.1.4",
  "module": "./dist/index.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.ts",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/lambda2/meshtastic-configs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lambda2/meshtastic-configs.git"
  },
  "type": "module",
  "author": "lambda2",
  "dependencies": {
    "@bufbuild/protobuf": "^1.10.0",
    "@meshtastic/js": "2.5.9-2",
    "deepmerge": "^4.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.2",
    "@types/chrome": "^0.0.263",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tslib": "^2.6.3",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.5.2"
  }
}